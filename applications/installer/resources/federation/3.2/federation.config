<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

	<entry key="CertificateAuthority.Identifier"></entry>
	<entry key="CertificateAuthority.Password">12345678</entry>
	<entry key="CertificateAuthority.File">#federation#.p7b</entry>

	<entry key="Certificate.File">federation.p12</entry>
	<entry key="Certificate.Password">1234</entry>

	<entry key="Jdbc.Database">#db_type#</entry>
	<entry key="Jdbc.DataSource">jdbc/FederationDatabase-#federation#</entry>

	<entry key="Socket.Host">127.0.0.1</entry>
	<entry key="Socket.Port">#socket_port#</entry>
	<entry key="Socket.MaxSessions">100</entry>
	<entry key="Socket.AllowedAddresses">127.0.0.1</entry> <!-- Ip addresses separated by comma -->

	<entry key="Smtp.Hostname">#mail_admin_host#</entry>
	<entry key="Smtp.Port">#mail_admin_port#</entry>
	<entry key="Smtp.User">#mail_admin_username#</entry>
	<entry key="Smtp.Password">#mail_admin_password#</entry>
	<entry key="Smtp.UseTLS">true</entry>
	<entry key="Smtp.UseSSL">true</entry>
	<entry key="Smtp.Email.From">#mail_admin_from#</entry>
	<entry key="Smtp.Email.To">#mail_admin_to#</entry>

	<entry key="Name">#federation#</entry>
	<entry key="Version">3.0.1</entry>
	<entry key="Autostart">true</entry>
	<entry key="InactivityTime">1200</entry> <!-- seconds -->
	<entry key="MaxRememberTime">10</entry> <!--  days   -->
	<entry key="MaxSessions">100</entry>
	<entry key="SuspendTime">900</entry> <!-- seconds -->
	<entry key="RemoveSuspendTime">7200</entry> <!-- seconds -->
	<entry key="SignTimestampInterval">60</entry> <!-- seconds -->

    <entry key="Mobile.PushApi.Key">AIzaSyBG6uhpiu7wxOzDH5qH08ZXe9WoMYWVwoE</entry>

</properties>
