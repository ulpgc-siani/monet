<div class="view dashboard" style="height:94%;">::view::</div>

@view.notloaded
<div class="node summary"><div class="message" style="clear:both;">'::labelDefinition::' aún no está cargado.</div></div>

@view.undefined
<div class="node summary"><div class="error" style="clear:both;">No se ha definido la vista '::codeView::' en la definición '::labelDefinition::'</div></div>

@view.olap
<iframe src="::analyticsUrl::?op=dashboard&id=::id::&lang=es&report=::codeView::&mode=embed" width="100%" height="100%" border="0" frameborder="0">Cargando...</iframe>

@view.report
<div class="message">¡Aún no implementado! Disculpe las molestias</div>