        </markerResolutionGenerator>
        <markerResolutionGenerator
            class="org.monet.editor.dsl.ui.MonetLocalizationLanguageExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="org.monet.editor.dsl.ui.monetlocalizationlanguage.check.normal">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
        <markerResolutionGenerator
            class="org.monet.editor.dsl.ui.MonetLocalizationLanguageExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
            markerType="org.monet.editor.dsl.ui.monetlocalizationlanguage.check.expensive">
            <attribute
                name="FIXABLE_KEY"
                value="true">
            </attribute>
        </markerResolutionGenerator>
    </extension>
