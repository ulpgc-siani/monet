					<xs:attribute name="workplace" type="key">
						<xs:annotation>
							<xs:documentation>WorkPlace donde comienza el workline</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="workstop" type="WorkstopDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="branch-lock" type="BranchLockDeclaration"></xs:element>
					<xs:element name="decision-lock" type="DecisionLockDeclaration"></xs:element>
					<xs:element name="form-lock" type="FormLockDeclaration"></xs:element>
					<xs:element name="service-lock" type="ServiceLockDeclaration"></xs:element>
					<xs:element name="sync-lock" type="SyncLockDeclaration"></xs:element>
					<xs:element name="timer-lock" type="TimerLockDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:sequence>
		<xs:attribute name="weight" type="natural">
