						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="generic" type="GenericDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="resolve" type="ResolveDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:sequence>
		<xs:attribute name="remote-link" type="string" use="required">
			<xs:annotation>
				<xs:documentation>Nombre de la definición remota a la que se conecta este proveedor</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="code" type="code" use="required">
			<xs:annotation>
				<xs:documentation>Nombre de la definición</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string" use="required">
			<xs:annotation>
				<xs:documentation>Nombre con el que se referencia una definición en el modelo de negocio</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="extends" type="key">
			<xs:annotation>
				<xs:documentation>Indica la definición de la que se heredan los atributos, propiedades y declaraciones</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TimerLockDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración de un bloqueo de temporizador. Estos bloqueos se resuelven automáticamente al cabo del tiempo indicado</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="timer" minOccurs="1">
				<xs:annotation>
					<xs:documentation>Configuración del temporizador</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="delay" type="integer" use="required">
						<xs:annotation>
							<xs:documentation>Duración del temporizador</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="extend" type="integer">
						<xs:annotation>
							<xs:documentation>???</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="is-main" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indica si este es el Worklock principal que el usuario debe resolver, en el caso de existir varios.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta del worklock</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="language" type="language" use="required">
								<xs:annotation>
									<xs:documentation>Idioma de la etiqueta</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="out" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="workstop" type="key">
						<xs:annotation>
							<xs:documentation>Salida del worklock</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="code" type="code" use="required">
			<xs:annotation>
				<xs:documentation>Nombre del campo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string" use="required">
			<xs:annotation>
				<xs:documentation>Nombre con el que se referencia la declaración en el modelo de negocio</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="WorklineDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar un workline de un workmap</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta del workline</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="language" type="language" use="required">
								<xs:annotation>
									<xs:documentation>Idioma de la etiqueta</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="result" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta de resultado del workline</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="language" type="language" use="required">
								<xs:annotation>
									<xs:documentation>Idioma de la etiqueta</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="from" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Define el origen del workline</xs:documentation>
				</xs:annotation>
