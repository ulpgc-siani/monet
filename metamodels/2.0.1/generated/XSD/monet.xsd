				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="language" type="language" use="required">
								<xs:annotation>
									<xs:documentation>Idioma de la descripción</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="workplace" type="WorkplaceDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="workline" type="WorklineDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WorkplaceDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar un workplace dentro de un workmap</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta del workplace</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="language" type="language" use="required">
								<xs:annotation>
									<xs:documentation>Idioma de la etiqueta</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
