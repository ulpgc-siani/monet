				<xs:restriction base="xs:string">
					<xs:pattern value="event|goal|dead-end"></xs:pattern>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="WorkStopDeclaration">
