						<xs:annotation>
							<xs:documentation>Idioma en el que se expresa la descripción</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="es|en|de|fr|it|pt"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="from" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Define la origen de este WorkLine</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="workplace" type="Code">
						<xs:annotation>
							<xs:documentation>WorkPlace desde el que parte esta línea</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="to">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0" maxOccurs="unbounded">
						<xs:choice>
							<xs:element name="workstop" type="WorkStopDeclaration"></xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="code" type="Code">
			<xs:annotation>
				<xs:documentation>Identificador único de la declaración del campo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="weight" type="Natural">
			<xs:annotation>
				<xs:documentation>Indica que tipo de WorkPlace se trata.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="WorkMapDeclaration">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
