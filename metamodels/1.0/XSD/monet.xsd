		<xs:attribute name="code" type="Code" use="required">
			<xs:annotation>
				<xs:documentation>WorkPlace where is placed this Workstop</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string">
			<xs:annotation>
				<xs:documentation></xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type">
			<xs:annotation>
				<xs:documentation>Indica que tipo de WorkPlace se trata.</xs:documentation>
			</xs:annotation>
