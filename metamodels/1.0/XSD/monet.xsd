						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="short|long"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="description" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Descripción explicativa de una definición. Se utiliza para ofrecer a los usuarios una explicación más detallada sobre un tipo de entidad</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma en el que se expresa la descripción</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="es|en|de|fr|it|pt"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="workplace" type="WorkPlaceDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
			<xs:sequence minOccurs="0" maxOccurs="unbounded">
				<xs:choice>
					<xs:element name="workline" type="WorkLineDeclaration"></xs:element>
				</xs:choice>
			</xs:sequence>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="WorkPlaceDeclaration">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
