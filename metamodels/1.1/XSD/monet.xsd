	<xs:complexType name="TextFieldDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar un campo de texto</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="allow-history" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						La presencia de esta propiedad indica que el campo
						puede gestionar un historial de los valores introducidos por el
						usuario
					</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="datastore" type="string" use="required">
						<xs:annotation>
							<xs:documentation>
								Nombre de la tabla en el que se almacenarán los
								valores. Dos campos que se encuentren incluso en formularios
								diferentes pueden compartir el mismo datastore
							</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="length" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se utiliza para definir la longitud máxima/mínima del campo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="max" type="natural">
						<xs:annotation>
							<xs:documentation>Máximo número de caracteres que admite el campo</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="min" type="natural">
						<xs:annotation>
							<xs:documentation>Mínimo número de caracteres que admite el campo</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="edition" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar cómo se guarda el texto: minúsculas, mayúsculas, oración o título</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="mode" use="required">
						<xs:annotation>
							<xs:documentation></xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="uppercase|lowercase|sentence|title"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="required" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar que es obligatorio cumplimentar el campo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="multiple" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Se utiliza para indicar que es un campo
						replicable en el formulario. Cuando un campo es replicable, en el
						espacio de negocio el usuario puede añadir instancias del campo, por
						ejemplo para introducir varios teléfonos en una ficha de datos
						personales
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extended" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar si un campo pertenece a la parte extendida de un campo sección</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="default-value" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Es el contenido por defecto de un campo cuando se crea el formulario</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta del campo. Se utiliza para mostrar a los usuarios el nombre del campo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la etiqueta</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="description" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Descripción del campo. Se utiliza para ofrecer a los usuarios una explicación más detallada del campo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la descripción</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="message" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Mensaje de ayuda para explicar al usuario el significado del campo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="type">
						<xs:annotation>
							<xs:documentation></xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="if-editing|if-empty|if-required"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma del mensaje</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="code" type="key" use="required">
			<xs:annotation>
				<xs:documentation>Código del campo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string">
			<xs:annotation>
				<xs:documentation>Nombre con el que se referencia un campo en el modelo de negocio</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
