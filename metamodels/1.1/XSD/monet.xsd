<?xml version="1.0" encoding="ISO-8859-1"?>
<xs:schema elementFormDefault="qualified" targetNamespace="http://www.monet.org" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.monet.org">
	<xs:complexType name="AttributeDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar un atributo de una referencia/cubo</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta del atributo. Se utiliza para mostrar a los usuarios el nombre del atributo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la etiqueta</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="code" type="key" use="required">
			<xs:annotation>
				<xs:documentation>Código del atributo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string" use="required">
			<xs:annotation>
				<xs:documentation>Nombre con el que se referencia un atributo en el modelo de negocio</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="type" use="required">
			<xs:annotation>
				<xs:documentation>Tipo del atributo</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="text|number|date|dimension"></xs:pattern>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="precision">
			<xs:annotation>
				<xs:documentation>Precisión de los atributos tipo fecha. Por defecto tiene precisión de segundos.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="years|months|days|hours|minutes|seconds"></xs:pattern>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="dimension">
			<xs:annotation>
				<xs:documentation>Nombre de la dimensión a la que está vinculado</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="years|months|days|hours|minutes|seconds"></xs:pattern>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BalanceScoreCardSectionDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar una sección del cuadro de mando</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta de la sección. Se utiliza para mostrar a los usuarios el nombre de la sección</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la etiqueta</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="description" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Descripción de la sección. Se utiliza para ofrecer a los usuarios una explicación más detallada de la sección</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la descripción</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="use" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>TODO</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="report" type="code">
						<xs:annotation>
							<xs:documentation>TODO</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="$tag">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0" maxOccurs="unbounded">
						<xs:choice>
							<xs:element name="section" type="BalanceScoreCardSectionDeclaration"></xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="name" type="string" use="required">
			<xs:annotation>
				<xs:documentation>Nombre de la sección del cuadro de mando</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="BalanceScorecardDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar un cuadro de mando integral</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="dashboard" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se utiliza para definir el panel del cuadro de mando</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="report" type="code">
						<xs:annotation>
							<xs:documentation>Código de la definición de un informe</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="$tag">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0" maxOccurs="unbounded">
						<xs:choice>
							<xs:element name="section" type="BalanceScoreCardSectionDeclaration"></xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CatalogDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar un catálogo</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="use" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="reference" type="code">
						<xs:annotation>
							<xs:documentation></xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="is-singleton" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Su presencia indica que sólo puede existir una instancia de este tipo de nodo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="is-component" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Su presencia indica que este tipo de nodo sólo se puede crear como componente de otro nodo, nunca de forma aislada</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="is-read-only" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Su presencia indica que el nodo es de sólo lectura</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="schema" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Esquema XML que representa la estructura del nodo. Permite operar con atributos del nodo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="export" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>La presencia de esta propiedad indica que se debe exportar el esquema a una referencia</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="reference" type="code">
						<xs:annotation>
							<xs:documentation>Código de la definición de una referencia</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="$tag">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0" maxOccurs="unbounded">
						<xs:choice>
							<xs:element name="parameter" type="ParameterDeclaration"></xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="$tag">
				<xs:annotation>
					<xs:documentation></xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence minOccurs="0" maxOccurs="unbounded">
						<xs:choice>
							<xs:element name="view" type="CatalogViewDeclaration"></xs:element>
						</xs:choice>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CatalogViewDeclaration">
		<xs:annotation>
			<xs:documentation></xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="show" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar que atributos de la referencia de la colección se van a mostrar en la vista</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="attribute" type="code" use="required">
						<xs:annotation>
							<xs:documentation>Código del atributo de la referencia</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="sort" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar que atributos de la referencia de la colección se van a utilizar para ordenar los elementos de la lista</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="attribute" type="code" use="required">
						<xs:annotation>
							<xs:documentation>Código del atributo de la referencia</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="group" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar que atributos de la referencia de la colección se van a utilizar para agrupar los elementos de la lista</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="attribute" type="code" use="required">
						<xs:annotation>
							<xs:documentation>Código del atributo de la referencia</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="select" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar qué nodos se muestran en la colección</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="node" type="code" use="required">
						<xs:annotation>
							<xs:documentation>Código de la definición de un nodo</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="filter" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar un filtro sobre el valor de un atributo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="attribute" type="code" use="required">
						<xs:annotation>
							<xs:documentation>Código del atributo de la referencia</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="operator" use="required">
						<xs:annotation>
							<xs:documentation>Operador que se utilizará para comprobar el filtro</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="eq|gt|ge|lt|le|neq"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="value" type="string" use="required">
						<xs:annotation>
							<xs:documentation>Valor con el que se comparará el atributo de la referencia. Se puede hacer referencia a un parámetro declarado en la colección</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta de la vista</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la etiqueta</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="code" type="key" use="required">
			<xs:annotation>
				<xs:documentation>Código de la vista</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string" use="required">
			<xs:annotation>
				<xs:documentation>Nombre de la vista</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CheckFieldDeclaration">
		<xs:annotation>
			<xs:documentation>Declaración que se utiliza para modelar un campo de comprobación</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="required" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar que es obligatorio cumplimentar el campo</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="multiple" minOccurs="0">
				<xs:annotation>
					<xs:documentation>
						Se utiliza para indicar que es un campo
						replicable en el formulario. Cuando un campo es replicable, en el
						espacio de negocio el usuario puede añadir instancias del campo, por
						ejemplo para introducir varios teléfonos en una ficha de datos
						personales
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="extended" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Se utiliza para indicar si un campo pertenece a la parte extendida de un campo sección</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="default-value" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Es el contenido por defecto de un campo cuando se crea el formulario</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="label" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Etiqueta del campo. Se utiliza para mostrar a los usuarios el nombre del campo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la etiqueta</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="description" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Descripción del campo. Se utiliza para ofrecer a los usuarios una explicación más detallada del campo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma de la descripción</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="message" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Mensaje de ayuda para explicar al usuario el significado del campo</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="type">
						<xs:annotation>
							<xs:documentation></xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="if-editing|if-empty|if-required"></xs:pattern>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attribute name="language" type="language" use="required">
						<xs:annotation>
							<xs:documentation>Idioma del mensaje</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="code" type="key" use="required">
			<xs:annotation>
				<xs:documentation>Código del campo</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="name" type="string">
			<xs:annotation>
				<xs:documentation>Nombre con el que se referencia un campo en el modelo de negocio</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
